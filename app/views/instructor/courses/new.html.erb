<br>
<div class="booyah-box col-xs-10 col-xs-offset-1 col-lg-6 col-lg-offset-3">
  <h1>Add a New Course</h1>
<br>
  <%= simple_form_for @course, url: instructor_courses_path do |t| %>
    <%= t.input :title %>
    <br>
    <%= t.input :description %>
    <br>
    <%= t.input :cost %>
    <br>
    <%= t.input :image %>
    <br>
    <%= t.submit "Create Course", class: "btn btn-success" %>
    <br>
  <% end %>
  <br>

</div>
<br>
<div class="booyah-box col-xs-10 col-xs-offset-1 col-lg-6 col-lg-offset-3">
  <h1>Add a New Lesson to Section: "<%= current_section.title %>"</h1>
<br>
  <%= simple_form_for @lesson, url: instructor_section_lessons_path(current_section) do |t| %>
    <%= t.input :title %>
    <br>
    <%= t.input :subtitle %>
    <br>
    <%= t.submit "Add Section", class: "btn btn-success" %>
    <br>
  <% end %>
  <br>

</div>